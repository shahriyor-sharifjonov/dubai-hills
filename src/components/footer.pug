                    a(href="#exterior").footer__link Exterior
                li.footer__li 
                    a(href="#interior").footer__link Interior
                li.footer__li 
                    a(href="#location").footer__link Location
                li.footer__li 
                    a(href="#community").footer__link Community
                li.footer__li 
